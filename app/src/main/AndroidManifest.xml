<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.damontung.healthclub">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/health_club"
        android:label="@string/app_name"
        android:theme="@android:style/Theme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShowAPILevel" />
        <activity
            android:name=".FWCActivity"
            android:label="@string/title_activity_fwc" />
        <activity
            android:name=".JuanFuActivity"
            android:label="@string/title_activity_juan_fu"
            android:process=":vitamio"
            ></activity>
    </application>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"></uses-permission>
</manifest>
